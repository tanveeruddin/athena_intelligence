# Database Configuration
DATABASE_URL=sqlite:///./data/asx_scraper.db

# Gemini API Configuration
GEMINI_API_KEY=AddYourAPIKeyHere
GEMINI_MODEL=gemini-2.0-flash-exp
GEMINI_TEMPERATURE=0.3
GEMINI_MAX_TOKENS=2048

# ASX Scraper Configuration
ASX_URL=https://www.asx.com.au/asx/v2/statistics/todayAnns.do
SCRAPE_ONLY_PRICE_SENSITIVE=true

# Storage Configuration
PDF_STORAGE_PATH=./data/pdfs
MARKDOWN_STORAGE_PATH=./data/markdown

# Memory Configuration
EPISODIC_MEMORY_LIMIT=10
SEMANTIC_MEMORY_UPDATE_THRESHOLD=3

# A2A Protocol Configuration
A2A_BASE_URL=http://localhost:8000
A2A_TIMEOUT_SECONDS=300

# Evaluation Configuration
ENABLE_EVALUATION=true

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Agent Configuration
COORDINATOR_AGENT_PORT=8000
SCRAPER_AGENT_PORT=8001
ANALYZER_AGENT_PORT=8002
STOCK_AGENT_PORT=8003
MEMORY_AGENT_PORT=8004
EVALUATION_AGENT_PORT=8005

# Testing Configuration
# Force evaluation agent to always return this recommendation (for testing human-in-the-loop)
# Valid options: BUY, SELL, HOLD, SPECULATIVE BUY, AVOID
# Leave commented out or empty for normal operation
FORCE_RECOMMENDATION_FOR_TESTING=BUY

# Phoenix Configuration
PHOENIX_ENABLED=true
PHOENIX_COLLECTOR_ENDPOINT=http://localhost:6006/v1/traces
PHOENIX_PROJECT_NAME=kaggle-google-adk-capstone